    <validationRules>
        <fullName>TASKRAY__Completing_Project_Restricted_to_Owner</fullName>
        <active>false</active>
        <errorConditionFormula>AND($User.Id !=  OwnerId ,ISCHANGED(TASKRAY__trCompleted__c),TASKRAY__trCompleted__c)</errorConditionFormula>
        <errorMessage>Only the Project Owner can mark the project as completed. Contact your system administrator if you would like access to this functionality.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TASKRAY__Editing_Score_Restricted_to_Owner</fullName>
        <active>false</active>
        <description>This validation rule prevents anyone except the project owner from modifying the Customer Onboarding Score.</description>
        <errorConditionFormula>AND( 
ISCHANGED( TASKRAY__trCustomerOnboardingScore__c ), 
NOT(ISBLANK(PRIORVALUE(TASKRAY__trCustomerOnboardingScore__c))), 
$User.Id != OwnerId 
)</errorConditionFormula>
        <errorMessage>Only the Project Owner can edit the Customer Onboarding Score. Contact your system administrator if you would like access to this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TASKRAY__External_Sharing_Restricted_to_Owner</fullName>
        <active>false</active>
        <description>This validation rule prevents anyone except the project owner from making a project available for external users</description>
        <errorConditionFormula>ISCHANGED( TASKRAY__trAvailableExternal__c ) &amp;&amp;
TASKRAY__trAvailableExternal__c == true &amp;&amp;
$User.Id !=  OwnerId</errorConditionFormula>
        <errorMessage>Only the Project Owner can make the project available for external sharing. Contact your system administrator if you would like access to this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TASKRAY__Feedback_Form_Restricted_to_Owner</fullName>
        <active>false</active>
        <description>This validation rule prevents anyone except the project owner from making a project available for external onboarding feedback.</description>
        <errorConditionFormula>AND(ISCHANGED( TASKRAY__trGenerateOnboardingFeedbackForm__c ) ,TASKRAY__trGenerateOnboardingFeedbackForm__c == true , $User.Id != OwnerId)</errorConditionFormula>
        <errorMessage>Only the Project Owner can make the project available for external onboarding feedback. Contact your system administrator if you would like access to this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TASKRAY__No_spaces_in_Nickname</fullName>
        <active>true</active>
        <errorConditionFormula>REGEX( TASKRAY__trNickname__c  ,&quot;.*\\s+.*&quot;)</errorConditionFormula>
        <errorMessage>Project Nicknames cannot have spaces</errorMessage>
    </validationRules>
